<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox spacing="5.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="empenofacil.controller.ComercializacionController">
   <children>
      <HBox alignment="CENTER_LEFT" spacing="5.0">
         <children>
            <Label text="Comercialización de prendas">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label text="Desde" />
            <DatePicker />
            <Label text="Hasta" />
            <DatePicker />
            <Button mnemonicParsing="false" text="Buscar" />
            <Button mnemonicParsing="false" text="Limpiar" />
            <Button mnemonicParsing="false" text="Convertir" />
         </children>
      </HBox>
      <TableView>
        <columns>
          <TableColumn editable="false" prefWidth="75.0" text="Nombre" />
          <TableColumn editable="false" prefWidth="75.0" text="Descripción" />
            <TableColumn editable="false" prefWidth="75.0" text="Avaluo" />
            <TableColumn editable="false" prefWidth="75.0" text="Prestamo" />
            <TableColumn prefWidth="75.0" text="Comercialización" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
